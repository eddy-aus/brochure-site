{
  "name": "brochure-site",
  "version": "0.2.2",
  "private": true,
  "description": "A brochure site for eddy",
  "homepage": "https://github.com/eddy-au/brochure-site#readme",
  "bugs": "https://github.com/eddy-au/brochure-site/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/eddy-au/brochure-site.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "bootstrap": "rm -rf node_modules && yarn install && husky install && chmod +x .husky/*",
    "bootstrap-frozen": "rm -rf node_modules && yarn install --frozen-lockfile && husky install && chmod +x .husky/*",
    "build": "rm -rf dist && webpack",
    "format": "sort-package-json && prettier --write \"**/*\"",
    "start": "webpack serve --env development"
  },
  "dependencies": {
    "on-the-case": "^2.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.15.3",
    "autoprefixer": "^10.3.7",
    "babel-loader": "^8.2.2",
    "cname-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.3.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.0",
    "postcss": "^8.3.9",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "sort-package-json": "^1.52.0",
    "style-loader": "^3.3.0",
    "webpack": "^5.57.1",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1"
  }
}
